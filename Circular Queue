#include<stdio.h> 
#define max  5 
 
int n,front=-1,i,ch,element,q[max],rear=-1; 
 
void  insert() 
{ 
 int data; 
if((rear==max-1 && front==0)||rear==front-1) 
{ 
   printf("Queue is full"); 
} 
else 
  { 
     printf("\nEnter data="); 
    scanf("%d",&data); 
 
  if (front==-1) 
 { 
   front++; 
   rear++; 
} 
else if(rear==max-1) 
{ 
   rear=0; 
} 
else 
{ 
   rear++; 
} 
q[rear]=data; 
printf("\nData inserted successfully"); 
} 
} 
 
void delet() 
{ 
  if (front==-1) 
{ 
   printf("Queue is empty"); 
} 
else 
{ 
   if(front==rear) 
  { 
     front=rear=-1; 
 } 
else if(front=max-1) 
{ 
    front=0; 
} 
else  
{ 
  front++; 
} 
printf("\nData deleted"); 
} 
} 
  
void display() 
{ 
   if(rear==-1&&front==-1) 
    { 
        printf("Queue is empty"); 
   } 
  else  
  { 
     printf("Queue elements are="); 
     if (rear<front) 
{ 
    for(i=front ;i<=max-1;i++) 
    { 
       printf("q[%d]=%d",i,q[i]); 
   } 
   for(i=0;i<=rear;i++) 
{ 
   printf("q[%d]=%d",i,q[i]); 
} 
} 
else  
{ 
   for(i=front;i<=rear;i++) 
{ 
   printf("q[%d]=%d",i,q[i]); 
} 
} 
} 
} 
 
void main() 
{ 
    do 
{ 
    printf("\n1.Insert\n2.delete\n3.Display\n4.Exitâ€); 
    printf("\nEnter your choice="); 
    scanf("%d",&ch); 
 
     switch(ch) 
{ 
  case 1: 
           insert(); 
          break; 
case 2: 
         delet(); 
         break; 
case 3: 
        display(); 
       break; 
case 4: 
        break; 
default: 
        printf("wrong choice"); 
break; 
} 
printf("\nDo you want to continue"); 
scanf("%d",&n); 
} 
 
while(n==0); 
} 
